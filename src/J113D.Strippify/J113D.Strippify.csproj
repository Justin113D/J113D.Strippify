<Project Sdk="Microsoft.NET.Sdk">

	<Import Project="../J113D.ProjectConfigurations/NuGet.Build.props" />

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<RootNamespace>J113D.Strippify</RootNamespace>
	</PropertyGroup>

	<PropertyGroup>
		<Title>J113D Triangle Strippifier library</Title>
		<Product>J113D Triangle Strippifier library</Product>
		<Description>Provides a triangle strippification algorithm</Description>
		<Version>1.0.0</Version>
		<RepositoryUrl>https://github.com/Justin113D/J113D.Strippify</RepositoryUrl>
		<PackageProjectUrl>https://github.com/Justin113D/J113D.Strippify</PackageProjectUrl>
		<PackageId>J113D.Strippify</PackageId>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="PublicAPI\net8.0\PublicAPI.Shipped.txt" />
	  <None Remove="PublicAPI\net8.0\PublicAPI.Unshipped.txt" />
	</ItemGroup>

	<ItemGroup>
		<None Include="../../README.md" Pack="true" PackagePath="" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="J113D.Common" Version="1.0.0" />
	</ItemGroup>

</Project>
